<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致
 -->
<mapper namespace="com.shop.mapper.Shopmapper">

	<!-- 根据用户的登录号和密码校验用户是否存在 -->
    <select id="CheckUserInfo" resultType="model.userinfo">
		SELECT 
			*
		FROM 
			userinfo
	</select>
	
	<select id="getvalue" resultType="com.shop.model.classinfo">
	    select * from classinfo where id = 3
	</select>
	
	<delete id="">
	</delete>
	
	<update id=""></update>
	
	<insert id=""></insert>
</mapper>